    "lg": "0.5rem",
    "xl": "0.75rem",
    "2xl": "1rem",
    "3xl": "1.5rem",
    "full": "9999px"
  },
  "borderWidth": {
    "default": "1px",
    "0": "0",
    "2": "2px",
    "4": "4px",
    "8": "8px"
  },
  "shadows": {
    "soft": "0 2px 8px rgba(0, 0, 0, 0.08)",
    "soft-lg": "0 4px 16px rgba(0, 0, 0, 0.1)",
    "soft-xl": "0 8px 24px rgba(0, 0, 0, 0.12)",
    "none": "none"
  },
  "transitions": {
    "duration": {
      "fast": "150ms",
      "normal": "200ms",
      "slow": "300ms",
      "slower": "500ms"
    },
    "timing": {
      "ease": "ease",
      "easeIn": "ease-in",
      "easeOut": "ease-out",
      "easeInOut": "ease-in-out",
      "linear": "linear"
    }
  },
  "components": {
    "button": {
      "primary": {
        "base": "bg-primary-600 text-white font-semibold px-8 py-4 rounded-full shadow-soft-lg transition-all duration-200",
        "hover": "hover:bg-primary-dark hover:text-[#f0fdf4] hover:shadow-soft-xl hover:-translate-y-1",
        "active": "active:translate-y-0 active:shadow-soft",
        "disabled": "disabled:opacity-50 disabled:cursor-not-allowed disabled:hover:translate-y-0 disabled:hover:shadow-none disabled:hover:bg-primary-600 disabled:hover:text-white"
      },
      "secondary": {
        "base": "bg-white border-2 border-gray-300 text-gray-700 font-semibold px-8 py-4 rounded-full transition-all duration-200",
        "hover": "hover:bg-gray-50 hover:border-gray-400 hover:-translate-y-1",
        "active": "active:translate-y-0",
        "disabled": "disabled:opacity-50 disabled:cursor-not-allowed"
      },
      "tertiary": {
        "base": "text-gray-500 hover:text-gray-700 underline text-sm transition-colors",
        "hover": "hover:text-gray-700",
        "active": "",
        "disabled": "disabled:opacity-50 disabled:cursor-not-allowed"
      },
      "bordered": {
        "base": "border-2 border-primary-500 text-primary-700 font-medium px-5 py-2.5 rounded-full transition-all duration-200",
        "hover": "hover:bg-primary-dark hover:text-[#f0fdf4]",
        "active": "active:translate-y-0.5",
        "disabled": "disabled:opacity-50 disabled:cursor-not-allowed"
      },
      "dashed": {
        "base": "border-2 border-dashed border-gray-300 text-gray-600 font-medium py-3 rounded-xl transition-all duration-200",
        "hover": "hover:border-primary-500 hover:text-primary-700 hover:bg-primary-50",
        "active": "active:translate-y-0.5",
        "disabled": "disabled:opacity-50 disabled:cursor-not-allowed"
      }
    },
    "input": {
      "base": "w-full px-4 py-3 rounded-xl border-2 outline-none transition-all font-medium",
      "default": "border-gray-200 focus:border-primary-500 focus:ring-4 focus:ring-primary-500/20",
      "error": "border-red-500 focus:border-red-500 focus:ring-4 focus:ring-red-500/20",
      "success": "border-primary-500 focus:border-primary-500 focus:ring-4 focus:ring-primary-500/20",
      "disabled": "disabled:bg-gray-100 disabled:cursor-not-allowed disabled:opacity-60"
    },
    "card": {
      "base": "bg-white rounded-2xl border-2 shadow-soft transition-all duration-300",
      "default": "border-emerald-100",
      "hover": "hover:border-primary-500 hover:-translate-y-2 hover:shadow-soft-lg",
      "active": "border-primary-500 bg-primary-50 ring-2 ring-primary-200",
      "error": "border-red-200 bg-red-50"
    },
    "badge": {
      "primary": "inline-block px-3 py-1 bg-primary-100 text-primary-700 rounded-full text-sm font-medium capitalize",
      "secondary": "inline-block px-3 py-1 bg-gray-100 text-gray-700 rounded-full text-sm font-medium capitalize",
      "success": "inline-flex items-center gap-1 px-2 py-1 bg-green-100 text-green-700 rounded-full text-xs font-medium",
      "error": "inline-flex items-center gap-1 px-2 py-1 bg-red-100 text-red-600 rounded-full text-xs font-medium",
      "warning": "inline-flex items-center gap-2 px-3 py-1.5 bg-amber-50 text-amber-700 rounded-lg text-sm font-medium",
      "info": "px-3 py-1 bg-blue-50 text-blue-700 rounded-full text-xs font-medium"
    },
    "checkbox": {
      "base": "w-5 h-5 rounded border-2 transition-all cursor-pointer",
      "default": "border-emerald-300 text-primary-600 focus:ring-4 focus:ring-primary-500/20",
      "checked": "border-primary-500 bg-primary-500"
    },
    "select": {
      "base": "w-full px-4 py-3 rounded-xl border-2 outline-none transition-all",
      "default": "border-gray-200 focus:border-primary-500 focus:ring-4 focus:ring-primary-500/20",
      "disabled": "disabled:bg-gray-100 disabled:cursor-not-allowed disabled:opacity-60"
    },
    "textarea": {
      "base": "w-full px-4 py-3 rounded-xl border-2 outline-none transition-all resize-none",
      "default": "border-gray-200 focus:border-primary-500 focus:ring-4 focus:ring-primary-500/20",
      "error": "border-red-500 focus:border-red-500 focus:ring-4 focus:ring-red-500/20"
    },
    "modal": {
      "overlay": "fixed inset-0 bg-black/30 backdrop-blur-sm z-40",
      "panel": "fixed top-0 right-0 bottom-0 w-full sm:w-[480px] bg-white shadow-2xl z-50 overflow-y-auto animate-in slide-in-from-right duration-300",
      "dialog": "fixed inset-0 bg-black/50 backdrop-blur-sm z-50 flex items-center justify-center p-4",
      "content": "bg-white rounded-3xl p-8 max-w-md w-full shadow-2xl animate-in zoom-in duration-300"
    },
    "loading": {
      "spinner": "animate-spin",
      "pulse": "animate-pulse",
      "bounce": "animate-bounce"
    }
  },
  "gradients": {
    "primary": "linear-gradient(135deg, #6cb257 0%, #4ade80 100%)",
    "warm": "linear-gradient(135deg, #fbbf24 0%, #f59e0b 100%)",
    "cool": "linear-gradient(135deg, #60a5fa 0%, #3b82f6 100%)"
  },
  "animations": {
    "fadeIn": {
      "keyframes": {
        "0%": { "opacity": "0" },
        "100%": { "opacity": "1" }
      },
      "duration": "300ms",
      "timing": "ease-in-out"
    },
    "slideInFromRight": {
      "keyframes": {
        "0%": { "transform": "translateX(100%)" },
        "100%": { "transform": "translateX(0)" }
      },
      "duration": "300ms",
      "timing": "ease-out"
    },
    "zoomIn": {
      "keyframes": {
        "0%": { "transform": "scale(0.95)", "opacity": "0" },
        "100%": { "transform": "scale(1)", "opacity": "1" }
      },
      "duration": "300ms",
      "timing": "ease-out"
    },
    "dash": {
      "keyframes": {
        "0%": { "transform": "rotate(0deg)" },
        "100%": { "transform": "rotate(360deg)" }
      },
      "duration": "2s",
      "timing": "linear",
      "iteration": "infinite"
    },
    "confettiBurst": {
      "keyframes": {
        "0%": { "transform": "translate(-50%, -50%) rotate(0deg) translate(0, 0) scale(0)", "opacity": "1" },
        "50%": { "opacity": "1" },
        "100%": { "transform": "translate(-50%, -50%) rotate(var(--angle)) translate(var(--distance), 0) scale(1)", "opacity": "0" }
      },
      "duration": "800ms",
      "timing": "ease-out"
    }
  },
  "scrollbar": {
    "thin": {
      "height": "6px",
      "track": {
        "background": "#d1fae5",
        "borderRadius": "3px"
      },
      "thumb": {
        "background": "#6ee7b7",
        "borderRadius": "3px",
        "hover": "#34d399"
      }
    }
  },
  "breakpoints": {
    "sm": "640px",
    "md": "768px",
    "lg": "1024px",
    "xl": "1280px",
    "2xl": "1536px"
  }
}
